function paymentsFormValues() {
  var ss = SpreadsheetApp.getActive(); 
  var sheet = ss.getSheetByName('PaymentsForm');
  var lastRow = sheet.getLastRow();
  var range = sheet.getRange('A:AM');
  var fieldManager = range.getCell(lastRow,5);
  var fieldValue = fieldManager.getValue();
  var jobNumber = range.getCell(lastRow,6);
  var jobNuValue = jobNumber.getValue();
  var jobName = range.getCell(lastRow,7);
  var jobNaValue = jobName.getValue();
  var company = range.getCell(lastRow,8);
  var companyValue = company.getValue();
  var reqDate = range.getCell(lastRow,11);
  var reqDateValue = reqDate.getValue();
  var workOneReq = range.getCell(lastRow,14);
  var workOneReqValue = workOneReq.getValue();
  var workOneArea = range.getCell(lastRow,15);
  var workOneAreaValue = workOneArea.getValue();
  var workOneAmount = range.getCell(lastRow,16);
  var workOneAmountValue = workOneAmount.getValue();
  var workOneComplete = range.getCell(lastRow,17);
  var workOneCompleteValue = workOneComplete.getValue();
  var workTwoReq = range.getCell(lastRow,19);
  var workTwoReqValue = workTwoReq.getValue();
  var workTwoArea = range.getCell(lastRow,20);
  var workTwoAreaValue = workTwoArea.getValue();
  var workTwoAmount = range.getCell(lastRow,21);
  var workTwoAmountValue = workTwoAmount.getValue();
  var workTwoComplete = range.getCell(lastRow,22);
  var workTwoCompleteValue = workTwoComplete.getValue();
  var workThreeReq = range.getCell(lastRow,24);
  var workThreeReqValue = workThreeReq.getValue();
  var workThreeArea = range.getCell(lastRow,25);
  var workThreeAreaValue = workThreeArea.getValue();
  var workThreeAmount = range.getCell(lastRow,26);
  var workThreeAmountValue = workThreeAmount.getValue();
  var workThreeComplete = range.getCell(lastRow,27);
  var workThreeCompleteValue = workThreeComplete.getValue();
  var workFourReq = range.getCell(lastRow, 29);
  var workFourReqValue = workFourReq.getValue();
  var workFourArea = range.getCell(lastRow,30);
  var workFourAreaValue = workFourArea.getValue();
  var workFourAmount = range.getCell(lastRow,31);
  var workFourAmountValue = workFourAmount.getValue();
  var workFourComplete = range.getCell(lastRow,32);
  var workFourCompleteValue = workFourComplete.getValue();
  var workFiveReq = range.getCell(lastRow,34);
  var workFiveReqValue = workFiveReq.getValue();
  var workFiveArea = range.getCell(lastRow,35);
  var workFiveAreaValue = workFiveArea.getValue();
  var workFiveAmount = range.getCell(lastRow,36);
  var workFiveAmountValue = workFiveAmount.getValue();
  var workFiveComplete = range.getCell(lastRow,37);
  var workFiveCompleteValue = workFiveComplete.getValue();
  
  
  var formSheet = ss.getSheetByName('PaymentsForm');
  var rowIdCell = formSheet.getRange('B1').setValue(lastRow);
  var fmCell = formSheet.getRange('B2').setValue(fieldValue);
  var jnCell = formSheet.getRange('B3').setValue(jobNuValue);
  var jnaCell = formSheet.getRange('B4').setValue(jobNaValue);
  var comCell = formSheet.getRange('B5').setValue(companyValue);
  var reqCell = formSheet.getRange('B6').setValue(reqDateValue);
  var workOneReqCell = formSheet.getRange('B8').setValue(workOneReqValue);
  var workOneAreaCell = formSheet.getRange('B9').setValue(workOneAreaValue);
  var workOneAmountCell = formSheet.getRange('B10').setValue(workOneAmountValue);
  var workOneCompleteCell = formSheet.getRange('B11').setValue(workOneCompleteValue);
  var workTwoReqCell = formSheet.getRange('B13').setValue(workTwoReqValue);
  var workTwoAreaCell = formSheet.getRange('B14').setValue(workTwoAreaValue);
  var workTwoAmountCell = formSheet.getRange('B15').setValue(workTwoAmountValue);
  var workTwoCompleteCell = formSheet.getRange('B16').setValue(workTwoCompleteValue);
  var workThreeReqCell = formSheet.getRange('B18').setValue(workThreeReqValue);
  var workThreeAreaCell = formSheet.getRange('B19').setValue(workThreeAreaValue);
  var workThreeAmountCell = formSheet.getRange('B20').setValue(workThreeAmountValue);
  var workThreeCompleteCell = formSheet.getRange('B21').setValue(workThreeCompleteValue);
  var workFourReqCell = formSheet.getRange('B23').setValue(workFourReqValue);
  var workFourAreaCell = formSheet.getRange('B24').setValue(workFourAreaValue);
  var workFourAmountCell = formSheet.getRange('B25').setValue(workFourAmountValue);
  var workFourCompleteCell = formSheet.getRange('B26').setValue(workFourCompleteValue);
  var workFiveReqCell = formSheet.getRange('B28').setValue(workFiveReqValue);
  var workFiveAreaCell = formSheet.getRange('B29').setValue(workFiveAreaValue);
  var workFiveAmountCell = formSheet.getRange('B30').setValue(workFiveAmountValue);
  var workFiveCompleteCell = formSheet.getRange('B31').setValue(workFiveCompleteValue); 
}
